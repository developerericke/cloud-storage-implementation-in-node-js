<body>

<!--navigation-->
<section class="smart-scroll">
  <div class="container-fluid">
    <nav class="navbar navbar-expand-md navbar-dark">
      <a class="navbar-brand heading-black" href="/">
        Mawingu Cloud
      </a>
      <button class="navbar-toggler navbar-toggler-right border-0" type="button" data-toggle="collapse"
              data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false"
              aria-label="Toggle navigation">
        <span data-feather="grid"></span>
      </button>
      <div class="collapse navbar-collapse " id="navbarCollapse">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link page-scroll" href="/authenticate">Login</a>
          </li>
          <li class="nav-item">
            <a class="nav-link page-scroll" href="/authenticate">Register</a>
          </li>

        </ul>
      </div>
    </nav>
  </div>
</section>

<!--hero header-->
<section class="py-7 py-md-0 bg-hero" id="home">
  <div class="container">
    <div class="row vh-md-100">
      <div class="col-md-8 col-sm-10 col-12 mx-auto my-auto text-center">
        <h1 class="heading-black text-capitalize">Cloud Storage Service for EveryOne</h1>
        <p class="lead py-3">Sign up for free and get your free 5GB cloud storage space.</p>
        <button class="btn btn-primary d-inline-flex flex-row align-items-center" onclick="window.location.href='/authenticate'">
          Get started now
          <em class="ml-2" data-feather="arrow-right"></em>
        </button>
      </div>
    </div>
  </div>
</section>



